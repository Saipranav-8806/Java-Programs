/*

* * * * *(5)
 *     *8
  *   *7
   * *6
    *5
    
*/

package java_app1;

public class HollowReverseTriangle {
	public static void main(String[] args) {
		int num=5;
		for(int i =num;i>0;i--) {
			for(int j = i;j<=num;j++) {
				System.out.print("o");
			}
			if(i>1) {
				for(int j =1;j<=2*i-1;j++) {
					if(j==1||j==2*i-1) {
						System.out.print("*");
					}
				}
				System.out.println();
			}
			
		}
		
	}

}
