*/

public class ButterFlyStar {
	public static void pattern(int num) {
		for (int i=0; i<num; i++) {
			System.out.print('*');
		}
		System.out.println();
		for (int i=num; i<num*2; i++) {
			System.out.print('*');
		}
		System.out.println();
	}
	public static void main(String[] args) {
		pattern(3);		
	}
}
