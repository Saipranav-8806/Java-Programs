/*
 1 2 3 4
  2 3 4
   3 4
    4
 */

import java.util.Scanner;

public class ReverseNumberTriangle {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter The Number: ");
		int num = sc.nextInt();
		
		for(int i = 1;i<=num;i++) {
			for(int j = 1;j<=i;j++) {
				for(int j=i;j<=num;j++ ) {
					System.out.print(j+" ");
				}
			}
			
			System.out.println();
		}
	}
	

}
