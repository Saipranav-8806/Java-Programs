import java.util.Scanner;

public class NumberIncreaseReverse {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter no: ");
		int num = sc.nextInt();
		for (int i = num;i>=0;i--) {
			for (int j =1 ;j<=num;j++) {
				System.out.print(j + "");
			}
			System.out.println();
		}			// TODO Auto-generated method stub
		

	}

}
