/*
    *		4,0	 
   * *  	3,1 
  *   *		2,3
 *     *	1,5  
* * * * *

 */
public class HollowTrianglePattern {

	public static void main(String[] args) {
		int num =5;
		for(int i = 0;i<num;i++) {
			for(int j =1;j<=num;j++) {
				System.out.print("l");
			}
		}
			
		
			System.out.println();
	}
}
