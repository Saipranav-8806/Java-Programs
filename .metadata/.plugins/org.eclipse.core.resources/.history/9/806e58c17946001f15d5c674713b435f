import java.util.Scanner;

public class K {
	public static void pattern(int num) {
		for(int i =0;i<=num*2;i++) {
			if(i<=num) {
				for(int j=1;j<=num;j++) {
					System.out.println("*");
				}
				
			}
			
		}
		
	}
	public static void main(String[] args) {
//		Scanner sc = new Scanner(System.in);
//		System.out.println("Enyer the number: ");
//		int num = sc.nextInt();
		pattern(4);
		
	}

}
