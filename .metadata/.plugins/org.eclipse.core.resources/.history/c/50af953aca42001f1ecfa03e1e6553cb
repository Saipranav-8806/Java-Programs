import java.util.Scanner;

public class NumberIncreaseReverse {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter no: ");
		int num = sc.nextInt();
		for (int i = 0;i<=num;i++) {
			for (int j = num;j>=0;j--) {
				System.out.print(j + "");
			}
		}			// TODO Auto-generated method stub
		System.out.println();

	}

}
